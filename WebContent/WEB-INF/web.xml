<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>JSP_Project_daEatdak</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
	<!-- 회원 -->
  <servlet>
  	<servlet-name>Member</servlet-name>
  	 <servlet-class>com.daeatdak.member.MemberFrontController</servlet-class>
  	<servlet-name>User</servlet-name>
  	<servlet-class>com.daeatdak.user.UserFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>*.me</url-pattern>
  </servlet-mapping>
	<!-- 게시판 -->
  <servlet>
  	<servlet-name>Board</servlet-name>
  	<servlet-class>com.daeatdak.board.BoardFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Board</servlet-name>
  	<url-pattern>*.bo</url-pattern>
  </servlet-mapping>
  
  	<!-- 상품 -->
  <servlet>
  	<servlet-name>Goods</servlet-name>
  	<servlet-class>com.daeatdak.goods.GoodsFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Goods</servlet-name>
  	<url-pattern>*.go</url-pattern>
  </servlet-mapping>
  
  	<!-- 관리자 -->
  <servlet>
  	<servlet-name>Admin</servlet-name>
  	<servlet-class>com.daeatdak.admin.AdminFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Admin</servlet-name>
  	<url-pattern>*.ad</url-pattern>
  </servlet-mapping>
  
    <!-- 메인 -->
  <servlet>
  	<servlet-name>Common</servlet-name>
  	<servlet-class>com.daeatdak.common.CommonFrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Common</servlet-name>
  	<url-pattern>*.co</url-pattern>
  </servlet-mapping>

   
</web-app>











